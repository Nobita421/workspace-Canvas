{
	"servers": {
		"sequentialthinking": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"-y",
				"@modelcontextprotocol/server-sequential-thinking@latest"
			],
			"gallery": true
		},
		"doist/todoist-ai": {
			"type": "http",
			"url": "https://ai.todoist.net/mcp",
			"gallery": "https://api.mcp.github.com",
			"version": "0.0.1-seed"
		},
		"supabase": {
			"command": "npx",
			"args": [
				"-y",
				"@supabase/mcp-server-supabase@latest",
				"--read-only",
				"--project-ref=qjwfckdyodwzktvcpwtl"
			],
			"env": {
				"SUPABASE_ACCESS_TOKEN": "sbp_ec897a1662c9230e5fd3f39fb44ef028912ec5d4"
			},
			"type": "stdio"
		},
		"memory": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"-y",
				"@modelcontextprotocol/server-memory@latest"
			],
			"env": {
				"MEMORY_FILE_PATH": "${input:memory_file_path}"
			},
			"gallery": true
		},
		"context7": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"-y",
				"@upstash/context7-mcp@latest"
			],
			"gallery": true
		},
		"microsoft-docs": {
			"type": "http",
			"url": "https://learn.microsoft.com/api/mcp",
			"gallery": true
		},
		"github": {
			"url": "https://api.githubcopilot.com/mcp/",
			"type": "http"
		},
		"firecrawl-local": {
			"type": "stdio",
			"command": "node",
			"args": [
				"D:\\movies\\firecrawl-mcp-server\\dist\\index.js"
			],
			"env": {
				"FIRECRAWL_API_KEY": "${input:firecrawl_api_key}"
			}
		},
		"sentry": {
			"url": "https://mcp.sentry.dev/mcp",
			"type": "http"
		},
		"codacy": {
			"command": "npx",
			"args": [
				"-y",
				"@codacy/codacy-mcp"
			],
			"env": {
				"CODACY_ACCOUNT_TOKEN": "${input:codacy_account_token}"
			},
			"type": "stdio"
		},
		"playwright": {
			"command": "npx",
			"args": [
				"@playwright/mcp@latest"
			],
			"type": "stdio"
		},
		"chromedevtools/chrome-devtools-mcp": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"-y",
				"chrome-devtools-mcp@latest"
			],
			"gallery": "https://api.mcp.github.com/v0/servers/13749964-2447-4c31-bcab-32731cced504",
			"version": "0.6.0"
		}
	},
	"inputs": [
		{
			"id": "api_key",
			"type": "promptString",
			"description": "your API key",
			"password": true
		},
		{
			"id": "browser_url",
			"type": "promptString",
			"description": "Optional: connect to an already-running Chrome (remote debugging / port-forward). Example: http://127.0.0.1:9222",
			"password": false
		},
		{
			"id": "headless",
			"type": "promptString",
			"description": "Run Chrome headless (true/false). Default: false",
			"password": false
		},
		{
			"id": "chrome_executable_path",
			"type": "promptString",
			"description": "Path to a specific Chrome/Chromium binary.",
			"password": false
		},
		{
			"id": "isolated",
			"type": "promptString",
			"description": "Use a temporary user-data-dir (true/false). Default: false",
			"password": false
		},
		{
			"id": "chrome_channel",
			"type": "promptString",
			"description": "Chrome channel: stable | canary | beta | dev (default: stable).",
			"password": false
		},
		{
			"id": "firecrawl_api_key",
			"type": "promptString",
			"description": "Firecrawl API Key (get from https://www.firecrawl.dev/app/api-keys)",
			"password": true
		},
		{
			"id": "memory_file_path",
			"type": "promptString",
			"description": "Path to the memory storage file",
			"password": false,
			"default": "D:\\movies\\new_pwa_c\\memory.json"
		},
		{
			"id": "codacy_account_token",
			"type": "promptString",
			"description": "Codacy Account Token for API access",
			"password": true
		}
	]
}